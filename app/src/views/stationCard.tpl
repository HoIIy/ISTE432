<div onclick="highlightMarker([@id])" class="station w3-container w3-btn w3-card-2 w3-margin-bottom w3-left-align" style="width: 100%">
    <div class="w3-cell-row w3-margin-top">
        <div class="w3-container w3-cell w3-third" style="padding:0;">
            <div class="w3-opacity street"><b>[@street]</b></div>
        </div>
        <!--div class="w3-container w3-cell w3-rest">
             
        </div-->
    </div>
    <h6 class="w3-text-teal"><i class="fa fa-clock-o fa-fw w3-margin-right"></i>[@access]</h6>
    <h6 class="w3-text-teal"><i class="fa fa-bolt fa-fw w3-margin-right"></i>[@fuel]</h6>
    <h6 class="w3-text-teal"><i class="fa fa-group fa-fw w3-margin-right"></i>[@group]</h6>
    <h6 class="w3-text-teal"><i class="fa fa-phone fa-fw w3-margin-right"></i>[@phone]</h6>
    <div><ul class="w3-ul w3-border">[@types]</ul></div>
    <div style="word-break:break-word;white-space:normal;"><h5>Directions:</h5><p>[@directions]</p></div>
    <div class="lat" style="display:none;">[@lat]</div>
    <div class="long" style="display:none;">[@long]</div>
    <div class="stationId" style="display:none;">[@id]</div>
</div>